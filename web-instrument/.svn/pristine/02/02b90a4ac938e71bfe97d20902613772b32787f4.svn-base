  line-height: 1.2;
  margin: 0;
  padding: 0;
}

.breadcrumbsCustom {
  margin-top: 0;
  margin-bottom: 0.5rem;
  margin-left: 0;
  padding: 0;
  list-style: none inside;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-wrap: nowrap;
      flex-wrap: nowrap;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
  background-color: #ffffff;
  color: #6a6a6a;
}

.breadcrumbsCustom .page-item,
.breadcrumbsCustom .breadcrumb-item {
  position: relative;
  cursor: pointer;
  display: list-item;
  background-color: inherit;
  color: inherit;
  -webkit-transition: all 0.15s ease-in-out;
  -o-transition: all 0.15s ease-in-out;
  transition: all 0.15s ease-in-out;
  margin: 4px;
}

.breadcrumbsCustom .page-item + .page-item::before,
.breadcrumbsCustom .breadcrumb-item + .breadcrumb-item::before,
.breadcrumbsCustom .page-item + .page-item::after,
.breadcrumbsCustom .breadcrumb-item + .breadcrumb-item::after {
  display: block;
  position: absolute;
  vertical-align: middle;
  color: transparent;
  font-size: 0;
  content: "";
  height: 1px;
  width: 0.375rem;
  background-color: #1d1d1d;
  top: 50%;
  left: 0;
  margin-left: -0.5rem;
}
.breadcrumbsCustom .page-item + .page-item::before,
.breadcrumbsCustom .breadcrumb-item + .breadcrumb-item::before {
  -webkit-transform: rotate(45deg);
      -ms-transform: rotate(45deg);
          transform: rotate(45deg);
  margin-top: -0.125rem;
}
.breadcrumbsCustom .page-item + .page-item::after,
.breadcrumbsCustom .breadcrumb-item + .breadcrumb-item::after {
  -webkit-transform: rotate(-45deg);
      -ms-transform: rotate(-45deg);
          transform: rotate(-45deg);
  margin-top: 0.125rem;
}
.breadcrumbsCustom .page-link,
.breadcrumbsCustom .page-item a,
.breadcrumbsCustom .breadcrumb-item a {
  display: block;
  position: relative;
  padding: 0.5rem 0.75rem;
  line-height: 1;
  font-size: 1rem;
  text-decoration: none;
  color: inherit;
  background-color: transparent;
}
.breadcrumbsCustom .page-link:hover,
.breadcrumbsCustom .page-item a:hover,
.breadcrumbsCustom .breadcrumb-item a:hover {
  color: #1d1d1d;
}
.breadcrumbsCustom .page-item:last-child,
.breadcrumbsCustom .breadcrumb-item:last-child {
  font-weight: bold;
}

